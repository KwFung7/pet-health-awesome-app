{
  "name": "AwesomeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "ENVFILE=.env.dev react-native run-android --appId com.awesome.member.dev --mode=devDebug",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios --scheme AwesomeAppDev",
    "start:dev": "react-native start  --reset-cache",
    "android:uat": "ENVFILE=.env.uat react-native run-android --appId com.awesome.member.uat --mode=uatRelease",
    "ios:uat": "ENVFILE=.env.uat react-native run-ios --scheme AwesomeAppUat",
    "start:uat": "ENVFILE=.env.uat react-native start --reset-cache",
    "android": "ENVFILE=.env.prod react-native run-android --appId com.awesome.member --mode=prodRelease",
    "ios": "ENVFILE=.env.prod react-native run-ios --scheme AwesomeApp",
    "start": "ENVFILE=.env.prod react-native start --reset-cache",
    "clean:android": "cd android && ./gradlew clean && ./gradlew cleanBuildCache",
    "clean:cache": "rm -rf node_modules/ && yarn cache clean && yarn install && cd android && ./gradlew clean && ./gradlew cleanBuildCache && cd ../ios && pod install && cd ..",
    "ios:install": "cd ios && pod install && cd ..",
    "android:build:dev": "cd android && ./gradlew clean bundleDevRelease && cd ..",
    "android:build:uat": "cd android && ./gradlew clean bundleUatRelease && cd ..",
    "android:build": "cd android && ./gradlew clean bundleProdRelease && cd ..",
    "test": "jest",
    "lint": "eslint . --ext .jsx --ext .js"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "axios": "^1.6.8",
    "i18next": "^23.11.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-i18next": "^14.1.1",
    "react-native": "0.74.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-config": "^1.5.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-portalize": "^1.0.7",
    "react-native-preference": "^2.2.3",
    "react-native-safari-view": "^2.1.0",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.7",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
